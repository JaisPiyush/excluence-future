{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate:scanner-store": "dotenv -- yarn workspace scanner-store migrate",
    "migrate:test_scanner-store": "dotenv -e .env.test -- yarn workspace scanner-store migrate",
    "test_run:scanner-store": "dotenv -e .env.test -- yarn workspace scanner-store run",
    "test:scanner-store": "dotenv -e .env.test -- yarn workspace scanner-store run test"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "@types/jest": "^29.5.2",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.6.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "turbo": "latest",
    "typescript": "^5.1.6"
  },
  "name": "flow-future",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
